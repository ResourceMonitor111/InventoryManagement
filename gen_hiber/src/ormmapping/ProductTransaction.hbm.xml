<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ProductTransaction" table="ProductTransaction" lazy="false">
		<composite-id>
			<key-many-to-one name="ORM_Productproduct" column="Productproduct_id" class="Product">
			</key-many-to-one>
			<key-many-to-one name="ORM_Transaction_transaction" column="Transaction_transaction_id" class="ShopTransaction">
			</key-many-to-one>
		</composite-id>
		<property name="sold_amount" column="sold_amount" type="integer" length="10" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
