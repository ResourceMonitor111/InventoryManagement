<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Clerk" table="Clerk" lazy="false">
		<id name="clerk_id" column="Clerk_id" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="firstname" column="firstname" type="string" length="255" not-null="true" lazy="false"/>
		<property name="lastname" column="lastname" type="string" length="50" not-null="true" lazy="false"/>
		<property name="contact_number" column="contact_number" type="integer" length="15" not-null="false" lazy="false"/>
		<property name="email" column="email" type="string" length="150" not-null="false" lazy="false"/>
		<set name="ORM_ShopTransaction" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="ClerkClerk_id" not-null="true"/>
			<one-to-many class="ShopTransaction"/>
		</set>
	</class>
</hibernate-mapping>
